		"password" : "1234",
		"id" : "read"
	},
	"MQTTService~write":
	{
		"brokerUrl" : "tcp://datainmotion.de:1883",
		"username" : "demo",
		"password" : "1234",
		"id" : "write"
	},
	"MQTTService~full":
	{
		"brokerUrl" : "ssl://$[env:MQTT_FULL_HOST;default=]$[prop:MQTT_FULL_HOST;default=mqttbr.jena.de]:$[env:MQTT_FULL_PORT;default=]$[prop:MQTT_FULL_PORT;default=8883]",
