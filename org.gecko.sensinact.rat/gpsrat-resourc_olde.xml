<resourceInfos xmlns="http://org.eclipse.sensinact/resource"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://org.eclipse.sensinact/resource ../../main/resources/sensinact-resource.xsd">
  <resourceInfo xsi:type="resourceInfoProperty" name="RATTE" target="sensor">
    <identifier xsi:type="stringContent">GPSRAT</identifier>
    <attributes>
    	<attribute name="value" type="float" modifiable="MODIFIABLE"> 
    		<value>5</value> 
		    <metadata>
			    <meta name="Description" type="string">
			        <value>modifiable gps sensor</value>
			    </meta>
			    <meta name="unit" type="string">
			        <value>celsius</value>
			    </meta>
			    <meta name="min" type="string">
			        <value>-25 C°</value>
			    </meta>
			    <meta name="max" type="string">
			        <value>+45 C°</value>
			    </meta>
		    </metadata>
		    <constraints>
		    	<minInclusive value="-25"/>
		    	<maxInclusive value="45"/>
		    </constraints>
    	</attribute>
    </attributes>
  </resourceInfo>
  <devices>
  	<device identifier="DIMC">
  		<service name="sensor"/>
  	</device>
  </devices>
  </resourceInfos>