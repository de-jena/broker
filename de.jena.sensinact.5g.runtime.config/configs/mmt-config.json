		"qvt.transformatorName": "ibisToSensinact",
		"qvt.model.target": "(&(emf.name=ibis)(emf.name=common)(emf.name=enumerations)(emf.name=customerinformationservice)(emf.name=gnsslocationservice)(emf.name=ticketvalidationservice))",
